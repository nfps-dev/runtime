  "license": "ISC",
  "author": {
    "name": "Blake Regalia",
    "email": "blake.regalia@gmail.com",
    "url": "https://blake-regalia.com/"
  },
  "type": "module",
  "main": "dist/main.mjs",
  "module": "dist/main.mjs",
  "types": "dist/main.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build:min": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build:dev": "yarn build:min --environment NODE_ENV:development",
    "build": "yarn build:min && yarn build:dev",
    "preview": "js-beautify dist/main.js > dist/main-inspect.js",
    "inline": "echo '<script type=\"application/ecmascript\">'\"\\n$(sed 's/&/\\&amp;/g; s/</\\&lt;/g; s/>/\\&gt;/g' dist/main.js)\\n</script>\" > dist/main-inline.xml",
    "dev": "yarn build:dev -w --environment NODE_ENV:development"
  },
  "devDependencies": {
    "@blake.regalia/eslint-config-elite": "^0.2.4",
    "@blake.regalia/tsconfig": "^0.0.2",
    "@nfps.dev/rollup-plugin-microweb": "^0.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.1.4",
    "@types/web": "^0.0.99",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "esbuild": "^0.17.19",
    "eslint": "^8.40.0",
    "eslint-plugin-i": "^2.27.5",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "js-beautify": "^1.14.7",
    "rollup": "^3.21.7",
    "rollup-plugin-filesize": "^10.0.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.0.4",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "@blake.regalia/belt": "^0.5.2",
    "@solar-republic/neutrino": "^0.0.1",
    "uri-js": "^4.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
