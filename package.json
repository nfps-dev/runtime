{
  "name": "@nfps.dev/runtime",
  "version": "0.1.3",
  "description": "Runtime library for NFPs",
  "keywords": [
    "nfp",
    "nfps.dev",
    "secret-network",
    "smart-contract",
    "nft",
    "privacy"
  ],
  "homepage": "https://nfps.dev/",
  "repository": "github:nfps.dev/runtime",
  "license": "ISC",
  "author": {
    "name": "Blake Regalia",
    "email": "blake.regalia@gmail.com",
    "url": "https://blake-regalia.com/"
  },
  "type": "commonjs",
  "main": "dist/cjs/main.js",
  "module": "dist/mjs/main.js",
  "types": "dist/mjs/main.d.ts",
  "exports": {
    "require": "./dist/cjs/main.js",
    "import": "./dist/mjs/main.js"
  },
  "files": [
    "dist/cjs",
    "dist/mjs"
  ],
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build:mjs": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "yarn build:mjs && yarn build:cjs",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "preview": "js-beautify dist/min/main.js > dist/min/main-inspect.js"
  },
  "devDependencies": {
    "@blake.regalia/eslint-config-elite": "^0.2.7",
    "@blake.regalia/tsconfig": "^0.1.2",
    "@nfps.dev/rollup-plugin-microweb": "^0.3.2",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.2.1",
    "@types/web": "^0.0.99",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "esbuild": "^0.17.19",
    "eslint": "^8.40.0",
    "eslint-plugin-i": "^2.27.5",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "js-beautify": "^1.14.7",
    "rollup": "^3.22.0",
    "rollup-plugin-filesize": "^10.0.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.0.4",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "@blake.regalia/belt": "^0.6.2",
    "@solar-republic/neutrino": "^0.1.4",
    "uri-js": "^4.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
