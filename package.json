{
  "name": "@nfps.dev/runtime",
  "version": "0.0.1",
  "description": "Runtime library for NFPs",
  "keywords": [
    "nfp",
    "nfps.dev",
    "secret-network",
    "smart-contract",
    "nft",
    "privacy"
  ],
  "homepage": "https://nfps.dev/",
